	AttackerStack [label="{AttackerStack}|attackerType : list\lattackDifferential : dictionary\lattackModifier : list\lcurrentRound : int\l|attack()\lcapture()\lapplyWounds()\lupdateCombatRating()\lupdateModifiers()\l" color=black fillcolor=lightgrey fontname=Helvetica fontsize=11 style=filled]
