	12 [label="This is an example annotation\l***\lList of important things:\l- Thing 1\l- Thing 2\l- And thing 3\l\lAggregates:\l- Possesion\l***\l" color=khaki4 fillcolor=khaki1 fontcolor=khaki4 fontname=Helvetica fontsize=11 shape=note style=filled]
