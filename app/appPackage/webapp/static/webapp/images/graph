	Mission [label="{Mission}|name : string\ltype : char\ldescription : string\l|actionEventCheck()\l" color=black fillcolor=lightgrey fontname=Helvetica fontsize=11 style=filled]
	Possession -> PossessionDeck [label=aggregate fontcolor=grey fontsize=9]
	Possession -> Character [label=Assigned arrowhead=none fontcolor=grey fontsize=9]
	Ship -> Possession [label=aggregate fontcolor=grey fontsize=9]
