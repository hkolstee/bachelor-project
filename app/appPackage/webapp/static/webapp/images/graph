	Ship [label="{Ship}|name : string\lcannons : int\lshields : int\lmaneuver : int\lmaxPassengers : int\l" color=black fillcolor=lightgrey fontname=Helvetica fontsize=11 style=filled]
	Companion [label="{Companion}|name : string\lcombatBonus : int\lintelligenceBonus : int\ldiplomacyBonus : int\lnavigationBonus : int\lspecial : list\l|getSpecial()\l" color=black fillcolor=lightgrey fontname=Helvetica fontsize=11 style=filled]
	Character [label="{Character}|name : string\ltitle : string\lcombat : int\lendurance : int\lintelligence : int\lleadership : int\ldiplomacy : int\lnavigation : int\lhomePlanet : string\leffectiveCombatRating : int\lwounds : int\lleader : boolean\ldetected : boolean\lcaptured : boolean\lactive : boolean\lbonusDraws : list\l|allegiance : string (Rebel or Imperial)\l" color=black fillcolor=lightgrey fontname=Helvetica fontsize=11 style=filled]
	IrateLocals [label="{IrateLocals}|type : string\lname : string\lcombat : int\lendurance : int\lhomePlanet : string\lattackType : string\leffectiveCombatRating : int\lwounds : int\l|getType()\l" color=black fillcolor=lightgrey fontname=Helvetica fontsize=11 style=filled]
